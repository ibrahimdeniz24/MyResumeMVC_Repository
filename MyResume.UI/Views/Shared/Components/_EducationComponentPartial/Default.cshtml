@using MyResume.UI.Areas.Admin.Models.AdminVMs.AdminEducationVMs;
@model List<AdminEducationListVM>

<h3 class="resume-title">Education</h3>
@foreach (var item in Model.OrderByDescending(m=>m.EndDateTime))
{

    <div class="resume-item">
        <h4>@item.Departman</h4>
        <h5>@item.StartDateTime.Value.ToShortDateString() - @item.EndDateTime.Value.ToShortDateString()</h5>
        <p><em>@item.School</em></p>
        <p>@Html.Raw(@item.Description)</p>
    </div>

    <!-- Edn Resume Item -->
}

