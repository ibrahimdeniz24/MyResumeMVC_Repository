@using MyResume.UI.Models.AdminVMs.AdminFeatureVMs
@model List<AdminFeatureListVM>
    


<section id="hero" class="hero section light-background">

    <img src="~/img/hero-bg2.jpg" alt="">

    <div class="container" data-aos="zoom-out">
        <div class="row justify-content-center">

            <div class="col-lg-9">

                @foreach (var item in Model)
                {

                    <h2>@item.Name</h2>
                    <p>
                        I'm <span class="typed" data-typed-items="@item.Title, @item.Title1, @item.Title2"></span><span class="typed-cursor typed-cursor--blink" aria-hidden="true"></span>
                    </p>
                }

                <div class="social-links">

                    <a href="https://www.linkedin.com/in/ibrahimdeniz24"><i class="bi bi-linkedin"></i></a>
                    <a href="https://medium.com/@@ibrahimdeniz24"><i class="bi bi-medium"></i></a>
                    <a href="https://www.github.com/ibrahimdeniz24"><i class="bi bi-github"></i></a>
                    <a href="https://www.instagram.com/developerwhiz"><i class="bi bi-instagram"></i></a>
                </div>
            </div>


        </div>
    </div>

</section>
